	<queryString language="SQL">
		<![CDATA[SELECT
     ANGGOTA_PINJAM."KD_ANGGOTAPINJAM" AS ANGGOTA_PINJAM_KD_ANGGOTAPINJA,
     ANGGOTA_PINJAM."KD_ANGGOTA" AS ANGGOTA_PINJAM_KD_ANGGOTA,
     ANGGOTA_PINJAM."KD_AKUN" AS ANGGOTA_PINJAM_KD_AKUN,
     ANGGOTA_PINJAM."KD_TENOR" AS ANGGOTA_PINJAM_KD_TENOR,
     ANGGOTA_PINJAM."TGL_PINJAM" AS ANGGOTA_PINJAM_TGL_PINJAM,
     ANGGOTA_PINJAM."NOMINAL_PINJAM" AS ANGGOTA_PINJAM_NOMINAL_PINJAM,
     ANGGOTA."NAMA_ANGGOTA" AS ANGGOTA_NAMA_ANGGOTA,
     TENOR."NAMA_TENOR" AS TENOR_NAMA_TENOR
FROM
     "KOPERASIWEB"."ANGGOTA" ANGGOTA INNER JOIN "KOPERASIWEB"."ANGGOTA_PINJAM" ANGGOTA_PINJAM ON ANGGOTA."KD_ANGGOTA" = ANGGOTA_PINJAM."KD_ANGGOTA"
     INNER JOIN "KOPERASIWEB"."TENOR" TENOR ON ANGGOTA_PINJAM."KD_TENOR" = TENOR."KD_TENOR"]]>
