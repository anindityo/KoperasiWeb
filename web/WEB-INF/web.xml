<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>TambahKaryawanServlet</servlet-name>
        <servlet-class>servlets.TambahKaryawanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TambahAnggotaServlet</servlet-name>
        <servlet-class>servlets.TambahAnggotaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TambahAnggotaSimpanServlet</servlet-name>
        <servlet-class>servlets.TambahAnggotaSimpanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TambahPemasukanKoperasiServlet</servlet-name>
        <servlet-class>servlets.TambahPemasukanKoperasiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TambahPinjamanServlet</servlet-name>
        <servlet-class>servlets.TambahPinjamanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAdminServlet</servlet-name>
        <servlet-class>servlets.EditAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditKaryawanServlet</servlet-name>
        <servlet-class>servlets.EditKaryawanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAnggotaServlet</servlet-name>
        <servlet-class>servlets.EditAnggotaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AngsuranServlet</servlet-name>
        <servlet-class>servlets.AngsuranServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TambahKaryawanServlet</servlet-name>
        <url-pattern>/tambahKaryawanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TambahAnggotaServlet</servlet-name>
        <url-pattern>/tambahAnggotaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TambahAnggotaSimpanServlet</servlet-name>
        <url-pattern>/tambahAnggotaSimpanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TambahPemasukanKoperasiServlet</servlet-name>
        <url-pattern>/tambahPemasukanKoperasiServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TambahPinjamanServlet</servlet-name>
        <url-pattern>/tambahPinjamanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAdminServlet</servlet-name>
        <url-pattern>/editAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditKaryawanServlet</servlet-name>
        <url-pattern>/editKaryawanServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAnggotaServlet</servlet-name>
        <url-pattern>/editAnggotaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AngsuranServlet</servlet-name>
        <url-pattern>/angsuranServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
